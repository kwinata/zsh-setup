alias gcl="git clone"
alias ga="git add"
alias gs="git status"
alias gd="git diff"
alias gdc="git diff --cached"
alias gc="git commit"
alias gres="git reset"
alias gb="git branch"
alias gco="git checkout"
alias gm="git merge"
alias gl="git log"
alias glone="git log --oneline"
alias glall="git log --oneline --graph --all"
alias gst="git stash"
alias gsty="git stash apply"
alias gt="git tag"
alias gf="git fetch"
alias gpl="git pull"
